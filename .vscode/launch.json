{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Preprocess",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/preprocess/bin/Debug/preprocess_dataset.exe",
            "args": [
                "--dataset", "${workspaceFolder}/generated_files/cubes_2.obj",
                "--output-geometry", "${workspaceFolder}/generated_files/cubes_2_geometry.txt",
                "--output-timing", "${workspaceFolder}/generated_files/cubes_2_preprocess_timing.json",
                "--generate-grid",
                "--grid-resolution", "10",
                "--world-size", "50",
                "--gamma", "1.0"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/generated_files",
            "environment": [
                {
                    "name": "PATH",
                    "value": "C:\\Users\\anton\\miniconda3\\envs\\rayspace3d_preprocess\\Library\\bin;C:\\Users\\anton\\miniconda3\\envs\\rayspace3d_preprocess\\Scripts;C:\\Users\\anton\\miniconda3\\envs\\rayspace3d_preprocess;${env:PATH}"
                }
            ],
            "console": "integratedTerminal",
            "preLaunchTask": "Build Preprocess"
        },
        {
            "name": "Query: Mesh Overlap",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/query/bin/Debug/raytracer_mesh_overlap.exe",
            "args": [
                "--mesh1", "${workspaceFolder}/generated_files/cubes_1_geometry.txt",
                "--mesh2", "${workspaceFolder}/generated_files/cubes_2_geometry.txt",
                "--output", "${workspaceFolder}/generated_files/cubes_overlap_timing.json"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/generated_files",
            "environment": [],
            "console": "integratedTerminal",
            "preLaunchTask": "Build Query"
        },
        {
            "name": "Query: Intersection Hash Table",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/query/bin/Debug/rayspace_intersection_estimated.exe",
            "args": [
                "--mesh1", "${workspaceFolder}/generated_files/cubes_1_geometry.txt",
                "--mesh2", "${workspaceFolder}/generated_files/cubes_2_geometry.txt",
                "--output", "${workspaceFolder}/generated_files/cubes_1_cubes_2_overlap_timing.json"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/generated_files",
            "environment": [],
            "console": "integratedTerminal",
            "preLaunchTask": "Build Query"
        },
        {
            "name": "Query: Intersection (Small DT)",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/query/bin/Debug/raytracer_mesh_intersection.exe",
            "args": [
                "--mesh1", "${workspaceFolder}/generated_files/simple_test_geometry.txt",
                "--mesh2", "${workspaceFolder}/generated_files/simple_test_geometry.txt",
                "--output", "${workspaceFolder}/generated_files/simple_test_intersection_timing.json"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/generated_files",
            "environment": [],
            "console": "integratedTerminal",
            "preLaunchTask": "Build Query"
        }
    ]
}